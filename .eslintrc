{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:css-modules/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import",
    "css-modules"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "globals": {
    "window": true,
    "document": true
  }
}